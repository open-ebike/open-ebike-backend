meta {
  name: Get bike passes for a Smart System bike
  type: http
  seq: 1
}

get {
  url: https://api.bosch-ebike.com/bike-pass/smart-system/v1/bike-passes?bikeId={{bikeId}}
  body: none
  auth: inherit
}

params:query {
  bikeId: {{bikeId}}
}

vars:pre-request {
  bikeId: xxxxxxxx-xxxx-xxxx-xxxx-xxxxxxxxxxxx
}

settings {
  encodeUrl: true
}
