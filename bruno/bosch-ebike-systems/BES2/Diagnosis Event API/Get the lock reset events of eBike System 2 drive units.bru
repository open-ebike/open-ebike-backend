meta {
  name: Get the lock reset events of eBike System 2 drive units
  type: http
  seq: 3
}

get {
  url: https://api.bosch-ebike.com/diagnosis-event/ebike-system-2/v1/lock-resets?partNumber={{partNumber}}&serialNumber={{serialNumber}}
  body: none
  auth: inherit
}

params:query {
  partNumber: {{partNumber}}
  serialNumber: {{serialNumber}}
}

vars:pre-request {
  partNumber: 12345678914
  serialNumber: 1707261/0447
}

settings {
  encodeUrl: true
}
